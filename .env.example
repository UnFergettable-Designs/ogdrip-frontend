# Frontend Local Development Environment Variables

# Backend URLs - Change these if your backend runs on a different port
BACKEND_URL=http://localhost:8888
PUBLIC_BACKEND_URL=http://localhost:8888

# Node Environment
NODE_ENV=development

# Port for the frontend development server
PORT=3000

# Astro Configuration
ASTRO_TELEMETRY_DISABLED=1

# Sentry Configuration (optional)
# SENTRY_DSN=
# SENTRY_ENVIRONMENT=development

# Sentry configuration for error tracking
PUBLIC_SENTRY_DSN=https://your-sentry-project-key@sentry.io/your-project-id
PUBLIC_SENTRY_ENVIRONMENT=production
PUBLIC_SENTRY_RELEASE=1.0.0
PUBLIC_SENTRY_DEBUG=false 