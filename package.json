{
  "name": "@ogdrip/frontend",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "astro dev --host",
    "dev:frontend": "astro dev --host",
    "start": "astro dev --host",
    "build": "astro build",
    "preview": "astro preview --host",
    "lint": "astro check && eslint . --ext .js,.ts,.astro",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm -rf .astro",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "validate": "node ./scripts/validate-imports.js"
  },
  "dependencies": {
    "@astrojs/node": "^9.1.3",
    "@astrojs/svelte": "^7.0.6",
    "@sentry/astro": "^9.5.0",
    "@sentry/browser": "^9.5.0",
    "astro": "^5.5.2",
    "esm-env": "^1.2.2",
    "svelte": "^5.23.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.7",
    "@types/node": "^22.13.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitest/coverage-v8": "^3.0.8",
    "eslint": "^9.22.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-svelte": "^3.1.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-svelte": "^3.3.3",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vitest": "^3.0.8"
  },
  "engines": {
    "node": ">=22.13.0"
  }
}
