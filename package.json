{
  "name": "@ogdrip/frontend",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "astro dev --host",
    "dev:frontend": "astro dev --host",
    "start": "astro dev --host",
    "build": "astro build",
    "preview": "astro preview --host",
    "lint": "astro check && eslint . --ext .js,.ts,.astro",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm -rf .astro",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "validate": "node ./scripts/validate-imports.js",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/node": "^9.1.3",
    "@astrojs/svelte": "^7.0.6",
    "@sentry/astro": "^9.5.0",
    "@sentry/browser": "^9.5.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "astro": "^5.4.3",
    "clsx": "^2.1.1",
    "esm-env": "^1.2.2",
    "p-limit": "^6.2.0",
    "svelte": "^5.23.0",
    "vite": "^6.2.1"
  },
  "devDependencies": {
    "@astrojs/svelte": "^5.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.7",
    "@types/node": "^22.13.10",
    "@vitest/coverage-v8": "^3.0.8",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.4.3",
    "svelte": "^5.0.0-next.62",
    "typescript": "^5.8.2",
    "vite": "^6.2.0",
    "vitest": "^3.0.8"
  },
  "engines": {
    "node": ">=18.14.1"
  }
}
